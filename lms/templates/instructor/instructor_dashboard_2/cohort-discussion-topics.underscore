<label><%- gettext('Coursewide Discussion Topics') %></label>
<div class="coursewide-discussion-topics">
    <% _.each(coursewideTopics, function(topic) { %>
        <input type="checkbox" class="cohorts-state" value="Cohorts-State" />
        <label for="cohorts-state"><%- topic.get('name') %></label>

    <% }); %>
</div>